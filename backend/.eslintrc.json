{
  "env": {
    "node": true,
    "es6": true,
		"jest": true
  },
  "extends": ["eslint:recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "ignorePatterns": ["node_modules/"],
  "rules": {
    "no-console": "off"
	}
}
